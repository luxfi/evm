#!/usr/bin/env bash

set -euo pipefail

# Ensure the go command is run from the root of the repository
REPO_ROOT=$(cd "$( dirname "${BASH_SOURCE[0]}" )"; cd .. && pwd )
cd "${REPO_ROOT}"

# Set LUX_VERSION
. ./scripts/versions.sh

echo "Running tmpnetctl @ ${LUX_VERSION}"
go run github.com/luxfi/node/tests/fixture/tmpnet/tmpnetctl@"${LUX_VERSION}" "${@}"
